    "froala-editor": "^4.0.7"
  },
  "devDependencies": {
    "css-loader": "^3.0.0",
    "html-loader": "^1.0.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^1.0.0",
    "url-loader": "^4.1.1",
    "wait-for-localhost": "^3.3.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.11.2",
    "resources": "file:../../../packages/res"
  }
