    "prestart": "yarn && yarn link @wiris/mathtype-froala && yarn link @wiris/mathtype-html-integration-devkit",
    "start": "webpack-dev-server --open"
  },
  "author": "WIRIS Team (https://www.wiris.com)",
  "license": "MIT",
  "dependencies": {
    "@wiris/mathtype-froala": "^7.27.0",
