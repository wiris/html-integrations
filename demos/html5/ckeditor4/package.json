{
  "name": "demo-html5-ckeditor4",
  "version": "1.1.0",
  "private": true,
  "description": "A simple html5 App integrating WIRIS MathType in a CKEditor4 rich text editor.",
  "main": "app.js",
  "scripts": {
    "prestart-remote": "yarn && yarn unlink @wiris/mathtype-ckeditor4 && yarn unlink @wiris/mathtype-html-integration-devkit && yarn install --force",
    "start-remote": "webpack-dev-server --open",
    "prestart": "yarn && yarn link @wiris/mathtype-ckeditor4 && yarn link @wiris/mathtype-html-integration-devkit",
    "start": "webpack-dev-server --open"
  },
  "author": "WIRIS Team (http://www.wiris.com)",
  "license": "MIT",
  "dependencies": {
    "@wiris/mathtype-ckeditor4": "^7.26.0",
    "@wiris/mathtype-html-integration-devkit": "*",
    "ckeditor4": "^4.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^5.0.0",
    "html-loader": "^1.0.0",
    "lerna": "^4.0.0",
    "raw-loader": "^4.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.0.0",
    "webpack-dev-server": "^3.0.0",
    "resources": "file:../../../packages/res"
  }
}
