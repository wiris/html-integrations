{
  "name": "demo-html5-ckeditor5",
  "version": "1.0.0",
  "description": "A simple html5 App integrating WIRIS MathType in a CKEditor5 rich text editor.",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "lerna bootstrap ; cd ../../../packages/mathtype-ckeditor5/ && filename=$( npm pack --quiet ) && cd - ; npm install ../../../packages/mathtype-ckeditor5/$filename ; webpack-dev-server -d --open",
    "start": "npm uninstall --save @wiris/mathtype-ckeditor5 && npm install --save @wiris/mathtype-ckeditor5 && webpack-dev-server --open",
    "deploy": "npm uninstall --save @wiris/mathtype-ckeditor5 && npm install --save @wiris/mathtype-ckeditor5 && webpack-dev-server --open",
    "build": "webpack --mode production",
    "lint": "npx eslint --quiet src/ webpack.config.js",
    "stylelint": "npx stylelint '**/*.css'",
    "linthtml": "html-validate index.html"
  },
  "author": "Carla Lara",
  "license": "MIT",
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": ">=19.0.1",
    "@ckeditor/ckeditor5-basic-styles": ">=19.0.1",
    "@ckeditor/ckeditor5-dev-utils": "^20.0.0",
    "@ckeditor/ckeditor5-editor-classic": ">=19.0.1",
    "@ckeditor/ckeditor5-essentials": ">=19.0.1",
    "@ckeditor/ckeditor5-paragraph": ">=19.1.0",
    "@ckeditor/ckeditor5-theme-lark": ">=19.1.0",
    "@wiris/mathtype-ckeditor5": "file:../../../packages/mathtype-ckeditor5/wiris-mathtype-ckeditor5-7.20.0.tgz",
    "postcss-loader": "^3.0.0"
  },
  "devDependencies": {
    "css-loader": "^3.5.3",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.21.2",
    "html-loader": "^1.1.0",
    "lerna": "^3.22.1",
    "raw-loader": "^4.0.1",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  }
}
