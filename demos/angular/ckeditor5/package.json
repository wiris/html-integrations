{
  "name": "demo-angular-ckeditor5",
  "version": "1.1.0",
  "description": "A simple Angular App integrating WIRIS MathType in a CKEditor rich text editor.",
  "scripts": {
    "prestart-remote": "yarn && yarn unlink @wiris/mathtype-ckeditor5 && yarn unlink @wiris/mathtype-html-integration-devkit && yarn install --force",
    "start-remote": "ng serve --host 0.0.0.0 --open",
    "prestart": "yarn && yarn link @wiris/mathtype-ckeditor5 && yarn link @wiris/mathtype-html-integration-devkit",
    "start": "ng serve --host 0.0.0.0 --open",
    "build": "ng build --prod --base-href ''",
    "lint": "ng lint"
  },
  "author": "WIRIS Team (http://www.wiris.com)",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@ckeditor/ckeditor5-angular": "^2.0.1",
    "@wiris/mathtype-ckeditor5": "*",
    "@wiris/mathtype-html-integration-devkit": "*"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.5",
    "@angular/animations": "^13.4.0",
    "@angular/cli": "^13.3.11",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/compiler-cli": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "^13.4.0",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "codelyzer": "^6.0.0",
    "resources": "file:../../../packages/res",
    "rxjs": "^6.6.0",
    "ts-node": "^8.3.0",
    "tslib": "^2.3.0",
    "tslint": "^6.1.0",
    "typescript": "4.6.4",
    "zone.js": "^0.11.4"
  }
}