{
  "name": "demo-html-ckeditor5",
  "version": "1.1.0",
  "private": true,
  "description": "A simple html App integrating WIRIS MathType in a CKEditor5 rich text editor.",
  "main": "app.js",
  "scripts": {
    "prestart-remote": "yarn && yarn unlink @wiris/mathtype-ckeditor5 && yarn unlink @wiris/mathtype-html-integration-devkit && yarn install --force",
    "prestart": "yarn && yarn link @wiris/mathtype-ckeditor5 && yarn link @wiris/mathtype-html-integration-devkit"
  },
  "author": "WIRIS Team (http://www.wiris.com)",
  "license": "MIT",
  "dependencies": {
    "@ckeditor/ckeditor5-alignment": ">=31.1.0",
    "@ckeditor/ckeditor5-basic-styles": ">=31.1.0",
    "@ckeditor/ckeditor5-dev-utils": ">=31.1.0",
    "@ckeditor/ckeditor5-editor-classic": ">=31.1.0",
    "@ckeditor/ckeditor5-essentials": ">=31.1.0",
    "@ckeditor/ckeditor5-paragraph": ">=31.1.0",
    "@ckeditor/ckeditor5-theme-lark": ">=31.1.0",
    "@wiris/mathtype-ckeditor5": "*",
    "@wiris/mathtype-html-integration-devkit": "*"
  },
  "devDependencies": {
    "css-loader": "^5.0.0",
    "html-loader": "^1.0.0",
    "raw-loader": "^4.0.0",
    "style-loader": "^2.0.0",
    "url-loader": "^4.0.0",
    "postcss-loader": "^4.0.0",
    "postcss": "^8.0.0",
    "resources": "file:../../../packages/res"
  }
}
