    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.0",
    "html-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "resources": "file:../../../packages/res",
