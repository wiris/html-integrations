    "coveralls": "^3.1.0",
    "css-loader": "^5.2.4",
    "jest": "^26.6.3",
    "jest-css-modules-transform": "^4.2.1",
    "jest-html-reporter": "^3.3.0",
    "jest-raw-loader": "^1.0.1",
    "puppeteer": "^9.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^5.1.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "raw-loader": "^4.0.2",
    "uuid": "^8.3.2"
  }
