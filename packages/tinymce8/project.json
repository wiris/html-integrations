{
  "name": "tinymce8",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "package/tinymce8/src",
  "targets": {
    "build": {
      "executor": "@nx/webpack:webpack",
      "dependsOn": [],
      "outputs": ["{options.outputPath}"],
      "options": {
        "main": "demos/packages/tinymce8/global.js",
        "outputPath": "dist",
        "tsConfig": "tsconfig.app.json",
        "generateIndexHtml": false,
        "index": " ",
        "webpackConfig": "packages/tinymce8/webpack.config.js"
      }
    },
    "start": {
      "executor": "@nx/webpack:dev-server",
      "options": {
        "buildTarget": "tinymce8:build",
        "webpackConfig": "packages/tinymce8/webpack.config.js"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "options": {
        "eslintConfig": "./.eslintrc.js",
        "lintFilePatterns": ["packages/tinymce8/**/*.{ts,tsx,js,jsx}"]
      },
      "outputs": ["{options.outputFile}"]
    }
  }
}
